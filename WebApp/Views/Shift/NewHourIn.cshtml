@model Services.Dtos.HorariosDto



@{
    ViewBag.Title = "NewHourIn";
}

<h2>Agregar horarios</h2>
<br />
<form action="NewHourIn" method="post">
    <input type="number" name="id" value="@Model.Id" hidden readonly /><br />
    <label>Id de empleado:</label>
    <input type="number" name="employeeId" value="@Model.EmployeeId" readonly /><br />
    <label>Horario de entrada:</label>
    <input type="time" name="checkIn" value="@Model.StartlHour.TimeOfDay" required/><br />
    <label>Horario de salida</label>
    <input type="time" name="checkOut" />
    <button type="submit" class="button">OK</button> 
</form>
